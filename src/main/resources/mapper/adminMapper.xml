<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.colin.secondkill.mapper.AdminMapper">


    <select id="selectUserByUsernameAndPassword" resultType="com.colin.secondkill.bean.Admin">
        SELECT * FROM t_admin WHERE admin_name = #{param1} AND password = #{param2}
    </select>
    <select id="getAllOrders" resultType="com.colin.secondkill.bean.Order">
        SELECT * FROM t_order
    </select>
</mapper>